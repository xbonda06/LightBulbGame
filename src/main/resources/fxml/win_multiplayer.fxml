    <HBox alignment="CENTER" prefHeight="63.0" prefWidth="223.0" spacing="10.0">
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <Button fx:id="ExitButton" alignment="CENTER" contentDisplay="CENTER" onAction="#toTheMain" prefHeight="40.0" prefWidth="177.0" styleClass="buttonVictory" stylesheets="@../css/styles.css" text="EXIT" />
    </HBox>
</VBox>
